{
  "name": "inventory-system",
  "version": "0.1.0",
  "description": "Convenient Store Inventory and POS System",
  "author": "mikeangelopolisticocabug@gmail.com",
  "private": true,
  "type": "module",
  "main": "mike.js",
  "homepage": "http://./",
  "scripts": {
    "start": "react-scripts start",
    "start-server": "cross-env NODE_EXTRA_CA_CERTS=./src/backend/certs/ca-bundle.pem node src/admin/backend/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-main": "electron mike.js",
    "electron-main-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron mike.js",
    "electron-delivery": "electron mike-delivery.js",
    "electron-delivery-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron mike-delivery.js",
    "dist:main": "electron-builder --config build.main.json",
    "dist:delivery": "electron-builder --config build.delivery.json",
    "dev": "concurrently \"npm start\" \"npm run electron-main-dev\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-list-endpoints": "^7.1.1",
    "framer-motion": "^12.23.22",
    "imap": "^0.8.19",
    "imap-simple": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "mailparser": "^3.7.4",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "nodemailer": "^7.0.9",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.3",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.31.0",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "ssl-root-cas": "^1.3.1",
    "web-vitals": "^5.1.0"
  },
  "build": {
    "appId": "com.inventory.system",
    "files": [
      "build/**/*",
      "mike.js",
      "mike-delivery.js",
      "loading.html",
      "src/admin/backend/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "keywords": [],
  "license": "ISC"
}
